{
  "name": "onolo-admin-dashboard",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:clean": "rm -rf node_modules/.vite dist && npm run dev",
    "dev:force": "rm -rf node_modules/.vite dist && VITE_CACHE_BUST=true npm run dev",
    "clear-cache": "./clear-dev-cache.sh",
    "deploy:prod": "./scripts/deploy-with-cache-bust.sh",
    "build": "tsc && vite build",
    "build:analyze": "tsc && vite build --mode analyze",
    "analyze": "vite build --mode analyze && npx vite-bundle-analyzer dist/stats.html",
    "preview": "vite preview",
    "type-check": "tsc --noEmit",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,json,css,md}\"",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "cap:sync": "npm run build && npx cap sync",
    "cap:sync:ios": "npm run build && npx cap sync ios",
    "cap:sync:android": "npm run build && npx cap sync android",
    "cap:open:ios": "npx cap open ios",
    "cap:open:android": "npx cap open android",
    "cap:run:ios": "npm run build && npx cap sync ios && npx cap open ios",
    "cap:run:android": "npm run build && npx cap sync android && npx cap open android"
  },
  "dependencies": {
    "@capacitor/android": "^7.4.3",
    "@capacitor/cli": "^7.4.3",
    "@capacitor/core": "^7.4.3",
    "@capacitor/ios": "^7.4.3",
    "@capacitor/push-notifications": "^7.0.3",
    "@headlessui/react": "^2.1.3",
    "@hookform/resolvers": "^3.9.0",
    "@supabase/supabase-js": "^2.45.3",
    "@tanstack/react-query": "^5.51.23",
    "clsx": "^2.1.1",
    "date-fns": "^3.6.0",
    "expo": "^54.0.13",
    "lucide-react": "^0.436.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.52.2",
    "react-hot-toast": "^2.4.1",
    "react-router-dom": "^6.26.1",
    "recharts": "^2.12.7",
    "tailwind-merge": "^3.3.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^8.45.0",
    "@typescript-eslint/parser": "^8.45.0",
    "@vitejs/plugin-react": "^4.3.1",
    "@vitest/ui": "^3.2.4",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.36.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.22",
    "install": "^0.13.0",
    "jsdom": "^26.1.0",
    "npm": "^11.6.1",
    "postcss": "^8.4.42",
    "prettier": "^3.6.2",
    "rollup-plugin-visualizer": "^6.0.4",
    "tailwindcss": "^3.4.10",
    "terser": "^5.44.0",
    "typescript": "^5.5.3",
    "undefined": "^0.1.0",
    "vite": "^5.4.2",
    "vitest": "^3.2.4"
  }
}
